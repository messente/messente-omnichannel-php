{
    "name": "messente/messente-omnichannel-php",
    "version": "0.0.1",
    "description": "PHP API client for Messente Omnichannel API",
    "keywords": [
        "php",
        "sdk",
        "api",
        "messaging",
        "sms",
        "viber",
        "omnichanenl"
    ],
    "homepage": "https://messente.com",
    "license": "Apache 2.0",
    "authors": [
        {
            "name": "Messente",
            "homepage": "https://github.com/messente/messente-omnichannel-php"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Messente\\Omnichannel\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Messente\\Omnichannel\\" : "test/" }
    }
}
